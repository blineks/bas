$=jQuery;var Project=Project||{};Project.initCarousel=function(){var rand_id;$(document).on('ready',function(){var homepageSwiper=new Swiper('.homepage-hero .swiper-container',{direction:'horizontal',loop:true,watchOverflow:true,pagination:{el:'.swiper-pagination',clickable:'true',},navigation:{nextEl:'.swiper-button-next',prevEl:'.swiper-button-prev',},});$('.in-page-swiper .swiper-container').each(function(index){swiper_id=$(this).data('swiper-id');lg_num_slides=$(this).data('num-slides');md_num_slides=typeof $(this).data('md-num-slides')!="undefined"?$(this).data('md-num-slides'):$(this).data('lg-num-slides');new Swiper('.in-page-swiper .swiper-container[data-swiper-id="'+swiper_id+'"]',{direction:'horizontal',loop:true,watchOverflow:true,centeredSlides:false,pagination:false,navigation:{nextEl:'.swiper-button-next[data-for-swiper="'+swiper_id+'"]',prevEl:'.swiper-button-prev[data-for-swiper="'+swiper_id+'"]',},slidesPerView:2,loopedSlides:2,breakpoints:{1070:{slidesPerView:lg_num_slides,loopedSlides:lg_num_slides},768:{slidesPerView:md_num_slides,loopedSlides:md_num_slides}}});});});};Project.initEllipsis=function(){$(document).ready(function(){$('.ellipsis').dotdotdot();});$(window).on('resize',function(){$('.ellipsis').dotdotdot();});};Project.initResponsiveEmbed=function(){$(document).ready(function(){$('iframe').wrap('<div class="video-container" />');});};$.fn.Next=function(){if($(this).is(':last-child')){return($(this).siblings().first());}else{return($(this).next());}};$.fn.Prev=function(){if($(this).is(':first-child')){return($(this).siblings().last());}else{return($(this).prev());}};$.expr[":"].Contains=$.expr.createPseudo(function(arg){return function(elem){return $(elem).text().toUpperCase().indexOf(arg.toUpperCase())>=0;};});String.prototype.replaceAll=function(search,replacement){var target=this;return target.replace(new RegExp(search,'g'),replacement);};Array.prototype.containsAny=function(arr){return this.some(function(v){return arr.indexOf(v)>=0;});};String.prototype.toProperCase=function(){return this.replace(/\w\S*/g,function(txt){return txt.charAt(0).toUpperCase()+txt.substr(1).toLowerCase();});};Project.fixedHeader=function(){$(window).on('scroll load',function(){if($(this).scrollTop()>45){$('.site-header').addClass('fixed');$('.hero').addClass('fixed-header');}else{$('.site-header').removeClass('fixed');$('.hero').removeClass('fixed-header');}});};Project.initForms=function(){$(document).ready(function(){if($('.select-content select').length>0){$('.select-content .select-item').first().addClass('active');}$('.select-content select').on('change',function(){$('.select-content .select-item').removeClass('active');$('#select-item-'+$(this).val()).addClass('active');});Project.initSelects();});};Project.initSelects=function(){var x,i,j,selElmnt,a,b,c;x=document.getElementsByClassName("select-wrapper");for(i=0;i<x.length;i++){selElmnt=x[i].getElementsByTagName("select")[0];a=document.createElement("DIV");a.setAttribute("class","select-selected");a.innerHTML=selElmnt.options[selElmnt.selectedIndex].innerHTML;x[i].appendChild(a);b=document.createElement("DIV");b.setAttribute("class","select-items select-hide");for(j=0;j<selElmnt.length;j++){c=document.createElement("DIV");c.innerHTML=selElmnt.options[j].innerHTML;c.addEventListener("click",function(e){var y,i,k,s,h;s=this.parentNode.parentNode.getElementsByTagName("select")[0];h=this.parentNode.previousSibling;for(i=0;i<s.length;i++){if(s.options[i].innerHTML==this.innerHTML){s.selectedIndex=i;$(s).trigger('change');h.innerHTML=this.innerHTML;y=this.parentNode.getElementsByClassName("same-as-selected");for(k=0;k<y.length;k++){y[k].removeAttribute("class");}this.setAttribute("class","same-as-selected");break;}}h.click();});b.appendChild(c);}x[i].appendChild(b);$(a).on('click',function(e){e.stopPropagation();closeAllSelect(e);this.nextSibling.classList.toggle("select-hide");this.classList.toggle("select-active");});}function closeAllSelect(elmnt){var x,y,i,arrNo=[];x=document.getElementsByClassName("select-items");y=document.getElementsByClassName("select-selected");for(i=0;i<y.length;i++){if(elmnt==y[i]){arrNo.push(i);}else{y[i].classList.remove("select-active");}}for(i=0;i<x.length;i++){if(arrNo.indexOf(i)){x[i].classList.add("select-hide");}}}document.addEventListener("click",closeAllSelect);};Project.initLoadMore=function(){$(document).ready(function(){$('.show-all').click(function(e){e.preventDefault();$(this).closest('.show-all-wrapper').find('.hide').removeClass('hide');$(this).hide();});});};Project.mobileNav=function(){$('.navbar-toggler').on('click',function(){if($(this).attr('aria-expanded')=='true'){$('.dropdown-menu').removeClass('active');}$(this).closest('header').toggleClass('active');setTimeout(function(){$('.navbar-collapse').height('auto');},500);});$('.site-header .navbar-nav > li a button').on('click',function(e){e.preventDefault();$(this).closest('li').find('.dropdown-menu').toggleClass('active');setTimeout(function(){$('.navbar-collapse').height($('.dropdown-menu.active > .submenu').height()+100);},500);});$('.site-header .back').on('click',function(e){e.preventDefault();$('.dropdown-menu').removeClass('active');setTimeout(function(){$('.navbar-collapse').height('auto');},500);});$(window).on('load resize',function(){});$('nav.footer-nav > .nav > li.menu-item-has-children > a').on('click',function(e){if($(window).width()<1070){e.preventDefault();if(!$(this).hasClass('active')){$('ul.sub-menu').slideUp();$('nav.footer-nav > .nav > li.menu-item-has-children > a').removeClass('active');}$(this).toggleClass('active');$(this).siblings('ul.sub-menu').slideToggle();}});};Project.splitHeight=function(){$(window).on('load resize',function(){});};Project.utilityToggle=function(){$('.utility-toggle').on('click',function(){$(this).children('.dropdown-menu').toggle();});$(window).on('keyup',function(e){if(e.keyCode==27){$('.utility-toggle').attr('open',false);}});$('#maincontent').on('click',function(){$('.utility-toggle').attr('open',false);});};Project.trackUTM=function(){$(document).on('ready',function(){var campaign=Project.getQueryParam('utm_campaign');var source=Project.getQueryParam('utm_source');var medium=Project.getQueryParam('utm_medium');var term=Project.getQueryParam('utm_term');if(campaign!=''&&campaign!=null){$.cookie('utm_campaign',campaign);}if(source!=''&&source!=null){$.cookie('utm_source',source);}if(medium!=''&&medium!=null){$.cookie('utm_medium',medium);}if(term!=''&&term!=null){$.cookie('utm_term',term);}if($('#utm_source').length>0&&($.cookie('utm_source')!='null'&&$.cookie('utm_source')!=null&&$.cookie('utm_source ')!="")){$('#utm_source').val($.cookie('utm_source'));}if($('#utm_medium').length>0&&($.cookie('utm_medium')!='null'&&$.cookie('utm_medium')!=null&&$.cookie('utm_medium ')!="")){$('#utm_medium').val($.cookie('utm_medium'));}if($('#utm_campaign').length>0&&($.cookie('utm_campaign')!='null'&&$.cookie('utm_campaign')!=null&&$.cookie('utm_campaign ')!="")){$('#utm_campaign').val($.cookie('utm_campaign'));}if($('#utm_term').length>0&&($.cookie('utm_term')!='null'&&$.cookie('utm_term')!=null&&$.cookie('utm_term ')!="")){$('#utm_term').val($.cookie('utm_term'));}});};Project.getQueryParam=function(name){url=window.location.href;name=name.replace(/[\[\]]/g,'\\$&');var regex=new RegExp('[?&]'+name+'(=([^&#]*)|&|#|$)'),results=regex.exec(url);if(!results)return null;if(!results[2])return'';return decodeURIComponent(results[2].replace(/\+/g,' '));}
